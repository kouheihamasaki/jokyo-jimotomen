<h1>新コミュニティ要望フォーム</h1>
  <%= form_with model: @community_req do |f| %>
    <h4>新しいコミュニティの名前</h4>
    <%= f.text_field :title %>
    <h4>どのようなコミュニティでしょうか？</h4>
    <%= f.text_area :body %>
    <%= f.hidden_field :update_status, :value => "incomplete" %>
    
    <%= f.submit '投稿' %>
    
    
  <% end %>
  